<h1><%= @spot.title %>
<h2><%= @spot.date %></h2>
<h2><%= @spot.user.user_name %></h2>
<%= image_tag @spot.image.url(:medium) %>
<% if user_signed_in? %>
<% if @spot.user.id == current_user.id %>
   <%= link_to "Edit", edit_spot_path(@spot) %>
<% else %>
  <%= link_to "Cancel", spots_path %>
<% end %>
<% end %>
